@model List<OrdinarioWebDefinitivoAhora.Models.MemoModel>

<div class="container mt-4">
    <h2 class="text-center mb-4">CONSULTA DE MEMORÁNDUM ACTIVOS</h2>

    <table class="table table-striped table-bordered">
        <thead class="table-dark">
            <tr>
                <th>Folio</th>
                <th>Año</th>
                <th>Fecha</th>
                <th>De</th>
                <th>Para</th>
                <th>Asunto</th>
                <th>Acciones</th>
            </tr>
        </thead>

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Folio</td>
                    <td>@item.FechaRegistro.ToString("dd/MM/yyyy")</td>
                    <td>@item.De</td>
                    <td>@item.Para</td>
                    <td>@item.Asunto</td>

                    
                </tr>
            }
        </tbody>
    </table>

    <a asp-controller="Home" asp-action="Index" class="btn btn-secondary mt-3">Regresar</a>
</div>
